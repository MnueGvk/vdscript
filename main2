        hitboxExpanderEnabled = v
        
        if v then
            startHitboxExpander()
            syncHitboxVisualWithExpander()
            Rayfield:Notify({
                Title = "Hitbox Expander",
                Content = "‚úÖ Activado - Hitboxes verdes en survivors",
                Duration = 3
