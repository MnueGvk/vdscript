    print("   Usa tu m√©todo actual pero con valores m√°s altos.")
    print("   Intenta con: GroupProject = 5, repairboost = 10.0")
    
elseif bestMethod == 2 then
    print("   El sistema responde a atributos extremos.")
    print("   A√±ade: RepairSpeed, IsRepairing")
    
elseif bestMethod == 3 then
    print("   Los atributos deben ir en el GENERADOR, no en el personaje.")
    print("   Usa: generator:SetAttribute() en lugar de char:SetAttribute()")
    
elseif bestMethod == 4 then
    print("   El progreso se puede manipular directamente.")
    print("   El sistema no valida fuertemente.")
    
else
    print("   El sistema tiene validaci√≥n fuerte.")
    print("   Necesitamos encontrar el RemoteEvent correcto.")
end

print("\nüîß C√ìDIGO RECOMENDADO BASADO EN AN√ÅLISIS:")
print([[```lua
-- Basado en an√°lisis, el mejor m√©todo es #]] .. bestMethod .. [[

local function applyOptimizedBoost()
    local char = LP.Character
    if not char then return end
    
    -- Encontrar generador cercano
    for _, gen in ipairs(workspace:GetDescendants()) do
        if gen:IsA("Model") and gen.Name:lower():find("generator") then
            local part = gen.PrimaryPart
            if part and (char.HumanoidRootPart.Position - part.Position).Magnitude < 20 then
                
                -- ]] .. (bestMethod == 1 and "M√©todo 1: Atributos b√°sicos (altos valores)" or 
                        bestMethod == 2 and "M√©todo 2: Atributos extremos" or
                        bestMethod == 3 and "M√©todo 3: Atributos en generador" or
                        "M√©todo 4: Manipulaci√≥n directa") .. [[
                
                ]] .. getMethodCode(bestMethod) .. [[
                
                break
            end
        end
    end
end
```]])

-- Funci√≥n para obtener el c√≥digo del mejor m√©todo
function getMethodCode(methodNum)
    if methodNum == 1 then
        return [[char:SetAttribute("GroupProject", 5)
                char:SetAttribute("gp_applied_by", 999999)
                char:SetAttribute("repairboost", 10.0)]]
    elseif methodNum == 2 then
        return [[char:SetAttribute("GroupProject", 10)
                char:SetAttribute("gp_applied_by", 999999)
                char:SetAttribute("repairboost", 50.0)
                char:SetAttribute("RepairSpeed", 50.0)
                char:SetAttribute("IsRepairing", true)]]
    elseif methodNum == 3 then
        return [[gen:SetAttribute("Repairers", 5)
                gen:SetAttribute("RepairSpeed", 10.0)
                gen:SetAttribute("ActiveRepairers", 5)
                gen:SetAttribute("RepairBoost", 10.0)]]
    else -- methodNum == 4
        return [[local progress = gen:GetAttribute("Progress") or 0
                if progress < 1 then progress = progress * 100 end
                gen:SetAttribute("Progress", math.min(100, progress + 25))]]
    end
end

